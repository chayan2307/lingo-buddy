<div class="card d-flex justify-content-start m-4" >
  <div class="container"id="message" <%= message.id %> >
    <small>
      <strong><%= message.user.first_name %></strong>
      <i><%= message.created_at.strftime("%a %b %e at %l:%M %p") %></i>
    </small>
    <p><%= message.content %></p>
  </div>
</div>
